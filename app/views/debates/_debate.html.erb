<div id="debate-<%= debate.id %>" class='debate'>
  <p><%= link_to debate.title, debate %></p>
  <p><%= debate.description %></p>

  <p>
    Creado el: <%= l debate.created_at.to_date %>
    por: <%= debate.author.name %>
  </p>

  <p><%= render 'shared/tags', debate: debate %></p>
</div>
