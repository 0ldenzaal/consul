<tr id="booth_<%= booth.id %>" class="booth">
  <td>
    <strong>
      <%= link_to booth.name, admin_booth_path(booth) %>
    </strong>
  </td>
  <td>
    <%= booth.location %>
  </td>
  <td class="text-right">
    <%= link_to t("admin.actions.edit"),
                edit_admin_booth_path(booth),
                class: "button hollow" %>
  </td>
</tr>